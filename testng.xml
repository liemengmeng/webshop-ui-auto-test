<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test verbose="2" preserve-order="true" name="登录注册">
        <classes>
            <class name="TestCase.openURL">
                <methods>
                    <include name="open"/>
                    <include name="closeAll"/>
                </methods>
            </class>
            <class name="TestCase.Login">
                <methods>
                    <include name="login1"/>
                    <include name="login2"/>
                    <include name="login3"/>
                </methods>
            </class>
            <class name="TestCase.forgetPWD">
                <methods>
                    <include name="passwordFalse"/>
                    <include name="passwordTrue"/>
                </methods>
            </class>
        </classes>
    </test>


    <test verbose="2" preserve-order="true" name="发布案例">
    <classes>
    <class name="TestCase.openURL">
        <methods>
            <include name="open"/>
            <include name="closeAll"/>
        </methods>
    </class>
    <class name="TestCase.ReleaseCase">
        <methods>
            <include name="cases"/>

        </methods>
    </class>
    </classes>
    </test>

</suite>